{
  "name": "@zomink/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build && pnpm postbuild",
    "postbuild": "pnpm with-env next-sitemap",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.29.5",
    "@trpc/client": "^10.32.0",
    "@trpc/next": "^10.32.0",
    "@trpc/react-query": "^10.32.0",
    "@trpc/server": "^10.32.0",
    "@zomink/api": "^0.1.0",
    "@zomink/auth": "^0.1.0",
    "@zomink/db": "^0.1.0",
    "@zomink/utilities": "^0.1.0",
    "formik": "^2.4.2",
    "next": "^13.4.4",
    "next-auth": "^4.22.1",
    "next-seo": "^6.1.0",
    "next-sitemap": "^4.1.3",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "sharp": "^0.32.1",
    "superjson": "1.9.1",
    "zod": "^3.21.4",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@zomink/eslint-config": "^0.1.0",
    "@zomink/tailwind-config": "^0.1.0",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3"
  }
}
